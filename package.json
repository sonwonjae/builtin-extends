{
  "name": "@sonwonjae/builtin-extends",
  "description": "A utility library that collects useful JavaScript built-in object extension methods.",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonwonjae/builtin-extends.git"
  },
  "keywords": [
    "builtin",
    "extends builtin",
    "extends built in object"
  ],
  "author": "sonwonjae",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonwonjae/builtin-extends/issues"
  },
  "homepage": "https://github.com/sonwonjae/builtin-extends#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf ./lib"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
